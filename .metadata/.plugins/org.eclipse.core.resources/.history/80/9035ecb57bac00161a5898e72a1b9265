package data.type.basic;

public class ImplicitTypeConversion {
	public static void main(String[] args) {
		char c1= 'a'; // 定义一个 char 类型
		int i1 = c1; // char 类型自动转换为 int
		System.out.println("char 类型自动转换为 int 后的值等于 " + i1);
		char c2 = 'A'; // 定义一个 char 类型
		int i2 = c2 + 1; // char 类型和  int 类型运算
		System.out.println("char 类型和 int 类型运算后的值等于 " + i2);
	}
}