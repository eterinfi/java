package struct.oop.inheritance;

interface A {
	public void eat();
	public void sleep();
}

interface B {
	public void show();
}

class C implements A, B {
	public void eat() { // 吃东西方法的具体实现
		System.out.println(name + " 正在吃");
	}
	public void sleep() {
		
	}
	public void show() {
		
	}
}

public class ImplementsDemo {
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
