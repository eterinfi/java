package data.object.operators;

public class AssignmentOpsDemo {
	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		c = a + b;
		System.out.println("c = a + b = " + c);
		c += a;
		System.out.println("c += a = " + c);
		c -= a;
		System.out.println("c -= a = " + c);
		c *= a;
		System.out.println("c *= a = " + c);
		c = 15;
		c /= a
	}

}
