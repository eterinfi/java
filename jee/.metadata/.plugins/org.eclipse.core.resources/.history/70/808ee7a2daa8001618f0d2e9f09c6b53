  <!-- 8. 使用filter输出Log -->
  <!-- 位置：LogFilter.java -->
  <filter>
    <filter-name>LogFilter</filter-name>
    <filter-class>com.runoob.test.LogFilter</filter-class>
    <init-param>
      <param-name>Site</param-name>
      <param-value>菜鸟教程</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>LogFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <!-- 9. 处理HTTP错误/异常信息 -->
